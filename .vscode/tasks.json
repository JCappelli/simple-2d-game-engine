{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "shell",
			"label": "Full Build",
			"command": "./windows_build.bat",
			"args": [
				"${workspaceFolder}"
			],
			"problemMatcher": [
				"$gcc"
			],
			"group": {
				"kind": "build"
			},
			"detail": "compiler: gcc from user PATH"
		},
		{
			"type": "shell",
			"label": "Code Only Build",
			"command": "./windows_build.bat",
			"args": [
				"${workspaceFolder}",
				"-c"
			],
			"problemMatcher": [
				"$gcc"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"detail": "compiler: gcc from user PATH"
		}
	]
}